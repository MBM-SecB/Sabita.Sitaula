@model Department

@{
    ViewData["Title"] = @Model.Name;
}

<h1>Department Detail <a href="/department/index">Go Back</a></h1>

<ul class="list-group list-group-flush">
    <li class="list-group-item">Name : @Model.Name</li>
    <li class="list-group-item">Code : @Model.Code</li>
    <li class="list-group-item">Description : @Model.Description</li>
    <li class="list-group-item">Start Date : @Model.StartDate</li>
</ul>